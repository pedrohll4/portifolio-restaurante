{
  "version": 2,
  "name": "restaurante-sabor-arte",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "js/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/sobre",
      "dest": "/sobre.html"
    },
    {
      "src": "/cardapio",
      "dest": "/cardapio.html"
    },
    {
      "src": "/carrinho",
      "dest": "/carrinho.html"
    },
    {
      "src": "/checkout",
      "dest": "/checkout.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
